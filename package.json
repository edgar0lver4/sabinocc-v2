{
  "name": "SabinoProyect",
  "version": "2.1.1",
  "private": true,
  "scripts": {
    "generate:dev-apk": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew assembleDebug && open ./app/build/outputs/apk/debug",
    "generate:prd-apk": "react-native bundle --platform android --mode=release --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && cd android && ./gradlew assembleRelease && open ./app/build/outputs/apk",
    "generate:bundle-prd": "cd android && ./gradlew clean && npx react-native build-android --mode=release && open ./app/build/outputs/bundle/release",
    "android": "cd android && ./gradlew clean && react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
